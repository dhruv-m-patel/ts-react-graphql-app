{
  "name": "ts-react-graphql-app",
  "version": "1.0.0",
  "description": "A universal react app with Typescript, GraphQL and material-ui support",
  "main": "build/server/index.js",
  "engines": {
    "node": ">= 14.15.5",
    "npm": ">= 6.14.11"
  },
  "scripts": {
    "build": "tsc && webpack",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "start-dev": "nodemon",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:ts": "eslint src/**/*.ts && eslint src/**/*.tsx",
    "lint:css": "stylelint \"./src/**/*.tsx\"",
    "lint": "npm run lint:ts && npm run lint:css",
    "lint:fix": "eslint --fix .",
    "pretest": "jest --clearCache",
    "prettier:format": "prettier --write .",
    "prettier:verify": "prettier --check .",
    "test": "jest --runInBand",
    "test:ci": "jest --silent --ci --coverage --coverageReporters=text --coverageReporters=text-summary",
    "test:changed": "jest --runInBand --onlyChanged"
  },
  "dependencies": {
    "@apollo/client": "^3.2.9",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.14.0",
    "@loadable/webpack-plugin": "^5.14.0",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.1",
    "@mui/styles": "^5.8.0",
    "apollo-server-express": "^2.25.3",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "clsx": "^1.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.1",
    "cors": "^2.8.5",
    "deepmerge": "^4.2.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "express-session": "^1.17.2",
    "fast-deep-equal": "^3.1.3",
    "fetch-everywhere": "^1.0.5",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "morgan": "^1.10.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^5.2.0",
    "store": "^2.0.12",
    "webpack": "^4.44.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.23",
    "@types/loadable__component": "^5.13.1",
    "@types/loadable__server": "^5.12.3",
    "@types/loadable__webpack-plugin": "^5.7.2",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.14.9",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-router-dom": "^5.1.6",
    "@types/store": "^2.0.2",
    "@types/supertest": "^2.0.11",
    "@types/testing-library__jest-dom": "^5.14.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "cli-glob": "^0.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.2.4",
    "jest-junit": "^12.2.0",
    "lint-staged": "^12.4.2",
    "nodemon": "^2.0.6",
    "prettier": "^2.6.2",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "supertest": "^6.1.3",
    "terser-webpack-plugin": "^4.2.3",
    "ts-jest": "^27.1.5",
    "ts-loader": "^8.4.0",
    "ts-node": "^10.8.0",
    "typescript": "4.7.2",
    "uuid": "^8.3.2",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-manifest-plugin": "^3.0.0",
    "webpack-node-externals": "^2.5.2"
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown",
      "eslint ."
    ]
  }
}
